<Window x:Class="Sens_6.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title= "Grap" Height="450" Width="800">
    <Grid>
        <d3:ChartPlotter Name="plotter">
            <d3:LineGraph Name="lineGraphReference_1" Stroke="Blue"/>
            <d3:LineGraph Name="lineGraphReference_2" Stroke="Red"/>
            <d3:LineGraph Name="lineGraphReference_3" Stroke="Orange"/>
            <d3:LineGraph Name="lineGraphReference_4" Stroke="Green"/>
            <d3:LineGraph Name="lineGraphReference_5" Stroke="Yellow"/>
            <d3:LineGraph Name="lineGraphReference_6" Stroke="Black"/>
        </d3:ChartPlotter>
    </Grid>
</Window>