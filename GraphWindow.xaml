<Window x:Class="Sens_6.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title= "Graph" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="7*"/>
            <ColumnDefinition Width="153*"/>
        </Grid.ColumnDefinitions>
        <d3:ChartPlotter Name="plotter" Grid.ColumnSpan="2">
            <d3:LineGraph Name="lineGraphReference_1" Stroke="Blue"/>
            <d3:LineGraph Name="lineGraphReference_2" Stroke="Blue"/>
            <d3:LineGraph Name="lineGraphReference_3" Stroke="Blue"/>
            <d3:LineGraph Name="lineGraphReference_4" Stroke="Blue"/>
            <d3:LineGraph Name="lineGraphReference_5" Stroke="Blue"/>
            <d3:LineGraph Name="lineGraphReference_6" Stroke="Blue"/>

            <d3:LineGraph Name="lineGraphTarget_1" Stroke="Red"/>
            <d3:LineGraph Name="lineGraphTarget_2" Stroke="Red"/>
            <d3:LineGraph Name="lineGraphTarget_3" Stroke="Red"/>
            <d3:LineGraph Name="lineGraphTarget_4" Stroke="Red"/>
            <d3:LineGraph Name="lineGraphTarget_5" Stroke="Red"/>
            <d3:LineGraph Name="lineGraphTarget_6" Stroke="Red"/>
        </d3:ChartPlotter>
    </Grid>
</Window>